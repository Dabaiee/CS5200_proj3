
<div class="row g-3">
    <% for (let coach of coaches) { %>

        <div class="col-4">
            <div class="card p-1">
                <h5 class="card-title"><%= coach.firstName+ " "+ coach.lastName %></h5>
                <div class="card-text">
                    <div>Coach ID:<%= coach.coachID %></div>
                    <div>Rating:<%= coach.ratings %> </div>
                </div>
            </div>
        </div>


    <% } %>

</div>
